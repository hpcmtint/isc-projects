package keaconfig

// Attention! Generated Code!
//
// Run "rake gen:std_option_defs" to regenerate the option definitions
// specified in the "codegen/std_dhcpv6_option_def.json" using the
// template file "stdoptiondef6.go.template" into the "stdoptiondef6.go".

// Returns standard DHCPv6 option definitions generated by the
// "stork-code-gen tool".
func getStdDHCPv6OptionDefs() []dhcpOptionDefinition {
	return []dhcpOptionDefinition{
        {{range .}}{
			Code:        {{.code}},
			Encapsulate: "{{.encapsulate}}",
			Name:        "{{.name}}",
			OptionType:  "{{.type}}",
			Space:       "{{.space}}",
            {{if index . "record-types"}}RecordTypes: []DHCPOptionType{
                {{range index . "record-types"}}"{{ . }}",{{end}}
            },{{end }}
		},{{end}}
	}
}
